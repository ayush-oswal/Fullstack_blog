
<%- include("partials/header") %>

<h1>Compose</h1>

<form action="/compose" method="post">
    <div class="form-group">
        <p>Title</p>
        <input class="form-control" type="text" name="title" autocomplete="off">
    </div>
    <div class="form-group">
        <p>Post</p>
        <textarea class="form-control" name="post" id="" cols="40" rows="5"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>


<%- include("partials/footer") %>